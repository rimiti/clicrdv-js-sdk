{
  "name": "clicrdv-js-sdk",
  "version": "0.0.1",
  "description": "ClicRDV Javascript SDK",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint .",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "babel src/**/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rimiti/clicrdv-js-sdk.git"
  },
  "keywords": [
    "clicrdv",
    "sdk",
    "api"
  ],
  "author": {
    "name": "Dimitri DO BAIRRO",
    "email": "dimitri.dobairro@dimsolution.com",
    "url": "dimsolution.com"
  },
  "bugs": {
    "url": "https://github.com/rimiti/clicrdv-js-sdk/issues"
  },
  "engines": {
    "node": ">=6.9"
  },
  "engineStrict": true,
  "homepage": "https://github.com/rimiti/clicrdv-js-sdk#readme",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.2.3",
    "babel-jest": "~22.4.4",
    "babel-plugin-transform-async-to-generator": "~6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.0",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-stage-3": "~6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "~4.19.1",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-flowtype": "~2.47.1",
    "eslint-plugin-import": "~2.12.0",
    "flow-bin": "~0.72.0",
    "jest": "~22.4.4"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
